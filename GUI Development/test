from socket import *
import pickle

HOST = "localhost"
PORT = 6000
Address = (HOST, PORT)
BUFSIZE = 1024

class TestClass:
    def __init__(self):
        self.name = "dogs"


clientSock = socket(AF_INET, SOCK_STREAM)
clientSock.connect(Address)



object = TestClass()

serializedObject = pickle.dumps(object)
#serializedObject = "cats"
print(serializedObject)

clientSock.send(serializedObject)